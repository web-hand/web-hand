{
  "name": "@web-hand/web-hand",
  "version": "0.0.1",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/web-hand.js",
  "modern": "dist/modern.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "type": "module",
  "author": "@web-hand",
  "license": "MIT",
  "homepage": "https://github.com/web-hand/web-hand",
  "bugs": {
    "url": "https://github.com/web-hand/web-hand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-hand/web-hand.git"
  },
  "scripts": {
    "postinstall": "git config core.hooksPath ./git-hooks",
    "serve": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettyprint": "prettier --check .",
    "prettyprint:fix": "prettier --write .",
    "test": "mocha --config-file .mocharc.json",
    "test:suite": "mocha --config-file .mocharc.json -g",
    "test:suite:watch": "mocha --config-file .mocharc.json --w -g",
    "test:watch": "mocha --config-file .mocharc.json --w --watch-files test/**/*.spec.ts",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --config-file .mocharc.json",
    "test:watch:coverage": "nyc --reporter=html --reporter=text mocha --config-file .mocharc.json --w --watch-files test/**/*.spec.ts",
    "test:external:coverage": "nyc --reporter=lcov --reporter=text mocha --config-file .mocharc.json"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc/core": "^1.2.211",
    "@swc/register": "^0.1.10",
    "@types/chai": "^4.3.1",
    "@types/jsdom": "^20.0.0",
    "@types/jsdom-global": "^3.0.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/sinon": "^10.0.12",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@mediapipe/hands": "^0.4.1646424915"
  },
  "optionalDependencies": {
    "@swc/core-darwin-x64": "^1.2.224"
  }
}
